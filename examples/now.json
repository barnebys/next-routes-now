{
  "routes": [
    {
      "src": "^\\/simple\\/route(?:\\/)?$",
      "dest": "/simple"
    },
    {
      "src": "^\\/complex\\/([^\\/]+?)(?:\\/)?$",
      "dest": "/complex?route=$1"
    },
    {
      "src": "^\\/multi\\/([^\\/]+?)(?:\\/([^\\/]+?))?(?:\\/)?$",
      "dest": "/multi?id=$1&slug=$2"
    }
  ]
}